Dispatch();\n  const [value, setValue] = React.useState([0, 100000]);\n const updateRange = (e, val) =>{\n   setValue(val)\n }\n\n  return (\n    <div className=\"filter-items\">\n      <div className=\"container1\">\n        <div className=\"filter-inside\">\n          <h3 className=\"filter\">Filter</h3>\n          <p>Min: {value[0]}</p>\n          <Slider \n          value={value}\n          max ={100000}\n          onChange={updateRange}\n          />\n          <p>Max: {value[1]}</p>\n        </div>\n        <div className=\"border1\"></div>\n        <div className=\"filter-inside\">\n          <h3 className=\"filter\">Brands</h3>\n          <div className=\"form-check\">\n           \n            <Checkbox name={'dell'} type='categary' />\n            <Checkbox name={'lenovo'} type='categary' />\n            <Checkbox name={'macbook'} type='categary'  />\n          </div>\n        </div>\n        <div className=\"border1\"></div>\n        <div className=\"filter-inside\">\n          <h3 className=\"filter\">RAM</h3>\n          <div className=\"form-check\">\n           \n            <Checkbox name={'8 gb'} type='ram' />\n            <Checkbox name={'4 gb'} type='ram' />\n          </div>\n        </div>\n        <div className=\"border1\"></div>\n        \n      </div>\n    </div>\n  );\n};\n\nexport default Filter;\n","/home/Sonu/Documents/Sonu/react-redux-flipkart/src/components/productCard/index.js",[],"/home/Sonu/Documents/Sonu/react-redux-flipkart/src/components/productCard/style.js",[],{"ruleId":"148","severity":1,"message":"149","line":10,"column":8,"nodeType":"150","messageId":"151","endLine":10,"endColumn":19},{"ruleId":"148","severity":1,"message":"152","line":11,"column":9,"nodeType":"150","messageId":"151","endLine":11,"endColumn":18},{"ruleId":"153","replacedBy":"154"},{"ruleId":"155","replacedBy":"156"},{"ruleId":"148","severity":1,"message":"157","line":5,"column":8,"nodeType":"150","messageId":"151","endLine":5,"endColumn":13},{"ruleId":"148","