Name={font ? 'nav-menu' : 'nav-menu active'}>\n        {MenuItems.map((item, index) => {\n          return (\n            <li key={index}>\n              <a className={item.cName} key={index} href={item.url}>\n                {item.title}\n              </a>\n            </li>\n          );\n        })}\n      </ul>\n      <Link to='/Kart'><div className=\"shopping-kart\"><i className=\"fas fa-shopping-cart\"></i></div></Link>\n     <p>{kartLogo.length}</p>\n    </nav>\n  );\n};\n\nexport default Navbar;\n",["128","129"],"/home/Sonu/Documents/Sonu/react-redux-flipkart/src/components/Navbar/MenuItems.js",[],"/home/Sonu/Documents/Sonu/react-redux-flipkart/src/components/Categaries/Categary.js",[],"/home/Sonu/Documents/Sonu/react-redux-flipkart/src/components/Mobile/Mobile.js",["130","131","132","133","134"],"/home/Sonu/Documents/Sonu/react-redux-flipkart/src/components/Laptop/Laptop.js",["135","136","137","138","139"],"/home/Sonu/Documents/Sonu/react-redux-flipkart/src/components/Mobile/Filter.js",["140"],"/home/Sonu/Documents/Sonu/react-redux-flipkart/src/reducers/index.js",[],"/home/Sonu/Documents/Sonu/react-redux-flipkart/src/actions/index.js",[],"/home/Sonu/Documents/Sonu/react-redux-flipkart/src/reducers/phone.js",[],"/home/Sonu/Documents/Sonu/react-redux-flipkart/src/reducers/data.js",[],"/home/Sonu/Documents/Sonu/react-redux-flipkart/src/reducers/combineData.js",[],"/home/Sonu/Documents/Sonu/react-redux-flipkart/src/reducers/mi.js",[],"/home/Sonu/Documents/Sonu/react-redux-flipkart/src/components/Kart/Kart.js",[],"/home/Sonu/Documents/Sonu/react-redux-flipkart/src/reducers/kart.js",[],"/home/Sonu/Documents/Sonu/react-redux-flipkart/src/components/Heart/Heart.js",[],"/home/Sonu/Documents/Sonu/react-redux-flipkart/src/components/Added/Added.js",["141"],"/home/Sonu/Documents/Sonu/react-redux-flipkart/src/components/phone-zoom/phoneZoom.js",[],"/home/Sonu/Documents/Sonu/react-redux-flipkart/src/reducers/phoneZoom.js",[],"/home/Sonu/Documents/Sonu/react-redux-flipka